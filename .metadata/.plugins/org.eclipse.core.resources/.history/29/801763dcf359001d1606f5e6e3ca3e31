{
    printf("AXI_GPIO Test!\n");

	axi_gpio_init();
	ps_led_init();
    while(1)
    {
    	if(key_flag)
    	{
    		key_flag=0;
    		led_val = ~led_val;
    		XGpioPs_WritePin(&gpiops, MIO_LED1, led_val);	//¸Ä±äLEDÁÁÃð
    	}
    }

